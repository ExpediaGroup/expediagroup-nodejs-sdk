{
  "name": "@expediagroup/fraudpreventionv2-sdk",
  "version": "2.0.0",
  "description": "ExpediaGroup FraudPreventionV2 SDK v2.0.0 Core Modules of EG Travel SDK",
  "main": "lib/index.js",
  "types": "index.ts",
  "scripts": {
    "build": "prettier --write '**/*.ts'",
    "clean": "rm -rf .openapi-generator apis auth http types",
    "fetch-core": "mkdir -p core/src && cp -r ../../../core/src ./core/ && cp -r ../../../core/index.ts ./core/index.ts",
    "generate-docs": "npx typedoc && mv ./docs/API.md ./docs/index.md",
    "merge-packages": "npm install -g merge-packages && npx merge-packages -o package.json package.json ../../../core/package.json",
    "prepare-sdk": "npm run merge-packages && npm run fetch-core && npm run clean && npm i && npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@types/node": "20.1.2",
    "@types/uuid": "9.0.1",
    "axios": "1.5.0",
    "typescript-json-serializer": "6.0.1",
    "uuid": "9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@types/jest": "29.5.4",
    "axios-mock-adapter": "1.22.0",
    "jest": "29.7.0",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "3.2.2",
    "ts-jest": "29.1.1",
    "ts-standard": "12.0.2",
    "typedoc": "0.25.0",
    "typedoc-plugin-markdown": "4.0.0-next.17",
    "typedoc-plugin-missing-exports": "2.1.0",
    "typescript": "5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "prepublish": "tsc"
}
